<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="Product.hbm.OutLib, Product" table="OutLib">
		<id name="ID" column="ID" type="integer"> 
			<generator class="identity" /> 
		</id> 
		<property name="User" column="UserName" type="String" length="50"/> 		
		<property name="OutTime" column="OutTime" type="DateTime"/>
		<property name="PumpName" column="PumpName" type="String" length="50"/>
		<property name="DrawType" column="DrawType" type="String" length="50"/>
		<property name="PumpType" column="PumpType" type="String" length="50"/>
	</class>
</hibernate-mapping>  
