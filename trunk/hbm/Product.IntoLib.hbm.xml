<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Product.IntoLib, Product" table="IntoLib">
		<id name="id" column="ID" type="integer"> 
			<generator class="native" /> 
		</id> 
		<property name="User" column="UserName" type="String" length="50"/> 		
		<property name="InputTime" column="InputTime" type="DateTime"/>
		<property name="PumpType" column="PumpType" type="String" length="50"/>
		<property name="PumpName" column="PumpName" type="String" length="50"/>
		<property name="DrawType" column="DrawType" type="String" length="50"/>
		<property name="FileName" column="FileName" type="String" length="255"/>
		<property name="Version"  column="Version"  type="integer"/>
		<property name="ProductNo" column="ProductNo" type="String" length="100"/>
	</class>
</hibernate-mapping> 
